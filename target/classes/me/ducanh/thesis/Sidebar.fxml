<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id = "sidebar"
            fx:controller="me.ducanh.thesis.SidebarController"
            >
            <VBox xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
            <children>
               <Button fx:id="addButton" mnemonicParsing="false" onAction="#addNode" text="V" />
               <Button fx:id="delButton" mnemonicParsing="false" onAction="#addEdge" text="E" />
               <Button fx:id="initButton" mnemonicParsing="false"  onAction="#init" text="T" />
               <Button fx:id="partitionButton" mnemonicParsing="false"  onAction="#part" text="part" />

            </children>
            </VBox>
</AnchorPane>
