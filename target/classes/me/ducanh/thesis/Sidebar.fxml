<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            id="sidebar"
            fx:id="sidebar"
            fx:controller="me.ducanh.thesis.SideBar"
>
    <VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1">
        <Button styleClass="sideBarButton" fx:id="aButton" mnemonicParsing="false" onAction="#vertices" text="Vertex"/>
        <Button styleClass="sideBarButton" fx:id="bButton" mnemonicParsing="false" onAction="#edges" text="Edge"/>
        <Button styleClass="sideBarButton" fx:id="cButton" mnemonicParsing="false" onAction="#randomEdges" text="Random Edges"/>
        <Button styleClass="sideBarButton" fx:id="dButton" mnemonicParsing="false" onAction="#bisimulation" text="Bisimulation"/>
        <Button styleClass="sideBarButton" fx:id="eButton" mnemonicParsing="false" onAction="#deltaFormula" text="Delta Formula"/>
        <Button styleClass="sideBarButton" fx:id="fButton" mnemonicParsing="false" onAction="#formulaSat" text="FormulaSat"/>

    </VBox>
</AnchorPane>
